{
  "name": "knowthen-calorie-counter-application",
  "version": "1.0.0",
  "description": "FP playground based on the knowthen calorie counter application.",
  "engines": {
    "node": "10.x.x",
    "npm": "6.x.x"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:stefanfrede/knowthen-calorie-counter.git"
  },
  "keywords": [
    "knowthen",
    "calorie",
    "counter",
    "calorie counter",
    "application"
  ],
  "author": {
    "name": "Stefan Frede",
    "email": "git@frede.io"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanfrede/knowthen-calorie-counter/issues"
  },
  "homepage": "https://github.com/stefanfrede/knowthen-calorie-counter",
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "Firefox ESR",
    "Opera 12.1"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "test": "if-ci npm run test:coverage && if-not-ci npm run test:watch",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "if-ci npm run test:e2e:run && if-not-ci npm run test:e2e:dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 cy:open",
    "build": "webpack --env production",
    "build:stats": "webpack --env production --json > stats.json",
    "dev": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --host 0.0.0.0 --env development\"",
    "start": "serve --no-clipboard --listen 8080 dist",
    "deploy": "gh-pages -d dist",
    "lint": "npm run lint:javascript && npm run lint:css",
    "lint:javascript": "eslint src/**/*.js",
    "lint:css": "stylelint src/**/*.css",
    "validate": "npm run lint && npm run test:coverage && npm run test:e2e:run"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "browserslist": "^4.5.6",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^2.0.1",
    "git-revision-webpack-plugin": "^3.0.3",
    "glob": "^7.1.4",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "husky": "^2.2.0",
    "if-ci": "^3.0.0",
    "jest": "^24.8.0",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.1.6",
    "lit-html": "^1.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.0",
    "npm-check-updates": "^3.1.9",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.17.0",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "sass-loader": "^7.1.0",
    "serve": "^11.0.0",
    "start-server-and-test": "^1.9.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-order": "^3.0.0",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/runtime-corejs2": "^7.4.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/pro-regular-svg-icons": "^5.8.2",
    "bulma": "^0.7.4",
    "crocks": "^0.11.1",
    "flyd": "^0.2.8"
  }
}
