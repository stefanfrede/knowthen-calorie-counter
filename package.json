{
  "name": "knowthen-calorie-counter-application",
  "version": "1.0.0",
  "description": "FP playground based on the knowthen calorie counter application.",
  "engines": {
    "node": "10.x.x",
    "npm": "6.x.x"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:stefanfrede/knowthen-calorie-counter.git"
  },
  "keywords": [
    "knowthen",
    "calorie",
    "counter",
    "calorie counter",
    "application"
  ],
  "author": {
    "name": "Stefan Frede",
    "email": "git@frede.io"
  },
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanfrede/knowthen-calorie-counter/issues"
  },
  "homepage": "https://github.com/stefanfrede/knowthen-calorie-counter",
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "Firefox ESR",
    "Opera 12.1"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,scss}": "stylelint",
    "*.js": "eslint",
    "*.{js,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "test": "if-ci npm run test:coverage && if-not-ci npm run test:watch",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "if-ci npm run test:e2e:run && if-not-ci npm run test:e2e:dev",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 cy:open",
    "build": "webpack --env production",
    "build:stats": "webpack --env production --json > stats.json",
    "build:start": "serve --no-clipboard --listen 8080 dist",
    "start": "nodemon --watch webpack.config.js --exec \"webpack-dev-server --env development\"",
    "deploy": "gh-pages -d dist",
    "lint": "npm run lint:javascript && npm run lint:css",
    "lint:javascript": "eslint src/**/*.js",
    "lint:css": "stylelint src/**/*.css",
    "validate": "npm run build && npm run lint && npm run test:coverage"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/register": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "browserslist": "^4.12.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "cypress": "^4.7.0",
    "cypress-testing-library": "^4.0.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-lit": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^3.0.0",
    "git-revision-webpack-plugin": "^3.0.6",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "if-ci": "^3.0.0",
    "jest": "^26.0.1",
    "jest-watch-typeahead": "^0.6.0",
    "lint-staged": "^10.2.9",
    "lit-html": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "purgecss-webpack-plugin": "^2.2.0",
    "serve": "^11.3.2",
    "start-server-and-test": "^1.11.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-order": "^4.1.0",
    "svg-inline-loader": "^0.8.2",
    "terser-webpack-plugin": "^3.0.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/pro-regular-svg-icons": "^5.13.0",
    "@tailwindcss/ui": "^0.3.0",
    "core-js": "^3.6.5",
    "crocks": "^0.12.4",
    "flyd": "^0.2.8",
    "tailwindcss": "^1.4.6"
  }
}
