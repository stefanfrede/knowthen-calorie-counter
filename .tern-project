{
  "ecmaVersion": 7,
  "libs": [
    "browser",
    "ecmascript"
  ],
  "loadEagerly": [
    "src/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "webpack": {}
  }
}
